    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "axios": "^1.6.3",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "lodash.debounce": "^4.0.8",
    "npm-run-all": "^4.1.5",
